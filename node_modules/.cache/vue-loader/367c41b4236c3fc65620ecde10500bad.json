               bottom: 0;\n                left: 50%;\n                transition: transform .3s ease;\n                transform: translate(-50%, -50%) matrix(1.00,0.00,0.00,1.00,0,3);\n            }\n        }\n\n        &__container {\n            opacity: 0;\n            pointer-events: none;\n            position: fixed;\n            z-index: -1;\n            top: 88px;\n            left: 0;\n            width: 100%;\n            height: calc(100vh - 88px);\n            background: #F7F8FA;\n            padding: 46px 35px;\n            transition: .3s opacity ease;\n\n            &:after {\n                content: '';\n                position: absolute;\n                z-index: 2;\n                top: 0;\n                width: calc(100% - 70px);\n                margin: 0 auto;\n                height: 1px;\n                background: rgba(148, 179, 219, 0.12);\n            }\n        }\n\n        &__nav {\n            position: absolute;\n            left: calc(50% - 5px);\n            transform: translateX(-50%);\n        }\n\n        &__list {\n            display: flex;\n\n            & > * {\n                &:not(:last-child) {\n                    margin-right: 55px;\n                }\n            }\n        }\n\n        &__item {\n            text-transform: uppercase;\n            font-size: 12px;\n            line-height: 19px;\n            text-align: center;\n            letter-spacing: 1px;\n            color: #37393E;\n            position: relative;\n            transition: .3s all ease;\n\n            &:hover {\n                color: #357FDB;\n            }\n\n            &:after {\n                content: '';\n                position: absolute;\n                top: 50%;\n                transform: translateY(-50%);\n                right: -20px;\n                width: 1px;\n                height: 14px;\n                background: rgba(190, 203, 217, 0.4);\n            }\n\n            &:last-child {\n                &:after {\n   