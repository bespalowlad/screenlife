n            el.select();                                    // Select the <textarea> content\n            document.execCommand('copy');                   // Copy - only works as a result of a user action (e.g. click events)\n            document.body.removeChild(el);                  // Remove the <textarea> element\n            if (selected) {                                 // If a selection existed before copying\n                document.getSelection().removeAllRanges();    // Unselect everything on the HTML document\n                document.getSelection().addRange(selected);   // Restore the original selection\n            }\n\n            for (let i = 0; i < success.length; i++) {\n                let el = success[i];\n                el.style.opacity = 1;\n    \n                setTimeout(function() {\n                    el.style.opacity = 0;\n                }, 3000)\n            }\n        },\n        replaceIcon(){\n            document.documentElement.clientWidth < 640 ? this.mobile = true : this.mobile = false;\n        }\n    },\n    mounted(){\n        window.addEventListener('resize', this.replaceIcon);\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n    .social-block {\n        position: relative;\n        z-index: 2;\n        display: flex;\n        align-items: center;\n\n        &__success {\n            position: absolute;\n            top: -55px;\n            right: auto;\n            left: 0;\n            width: 162px;\n            height: 36px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            opacity: 0;\n            transition: 0.3s all ease;\n\n            &:after {\n                content:'';\n                position: absolute;\n                z-index: -1;\n                top: 0;\n                left: 0;\n                width: 100%;\n                height: 100%;\n                // background: rgba(255, 255, 255, 0.38);\n                background: #ecf3fd;\n                // opacity: 0.5;\n                // border: 1px solid rgba(0, 0, 0, 0.02);\n                box-sizing: border-box;\n                box-shadow: 0px 4px 29px rgba(52, 59, 76, 0.05);\n                border-radius: 3px;\n            }\n\n            svg {\n                margin-right: 13px;\n\n                path {\n                    fill: #7BAAE6;\n                }\n            }\n\n            p {\n                font-family: 'Roboto';\n                font-style: normal;\n                font-weight: 300;\n                font-size: 12px;\n                line-height: 25px;\n                color: #7BAAE6;\n            }\n        }\n\n        &__share {\n            display: inline-block;\n            font-family: 'SFProDisplay';\n            font-weight: normal;\n            font-size: 12px;\n            line-height: 19px;\n            letter-spacing: 1px;\n            text-transform: uppercase;\n            margin-right: 16px;\n            color: #37393E;\n        }\n\n        &__list {\n            display: flex;\n            align-items: center;\n\n            & > * {\n                &:not(:last-child) {\n                    margin-right: 13px;\n                }\n            }\n        }\n\n        &__item {\n            &.fix {\n                padding-left: 10px;\n                position: relative;\n\n                &:before {\n                    content: '';\n                    width: 1px;\n                    height: 11px;\n                    position: absolute;\n                    left: 0;\n                    top: 50%;\n                    transform: translateY(-50%);\n                    background: #37393E;\n                    opacity: 0.2;\n                }\n            }\n\n            svg path {\n                transition: .3s all ease;\n                fill: #7BAAE6;\n            }\n\n            @media screen and (min-width: 1200px) {\n\n                &:hover {\n                    svg path {\n                        fill: #4F7DB7;\n                    }\n                }\n            }\n        }\n\n        &.light {\n\n            .social-block {\n                &__success {\n                    left: auto;\n                    right: 0;\n                    \n                    &:after {\n                        background: rgba(255, 255, 255, 0.38);\n                    }\n\n                    svg {\n                        path {\n                            fill: #fff;\n                        }\n                    }\n\n                    p {\n                        color: #fff;\n                    }\n                }\n\n                &__share {\n                    color: #fff;\n                }\n\n                &__item {\n                    svg path {\n                        transition: .3s all ease;\n                        fill: #fff;\n                    }\n\n                    &.fix {\n                        &:before {\n                            background:#fff;\n                        }\n                    }\n\n                    @media screen and (min-width: 1200px) {\n\n                        &:hover {\n                            svg path {\n                                opacity: 0.8;\n                            }\n                        }\n                    }\n                }\n            }\n        }\n        &_absolute {\n            white-space: nowrap;\n            display: inline-block;\n            position: absolute;\n            right: 71px;\n            bottom: 33px;\n            -webkit-box-pack: unset;\n            justify-content: unset;\n            margin-right: 0;\n            .social-block {\n                &__list {\n                    display: inline-block;\n                    li {\n                        display: inline-block;\n                    }\n                }\n            }\n        }\n    }\n\n    @media (max-width: 960px) {\n        .social-block {\n            &__success {\n                right: auto;\n                left: 0;\n            }\n\n            &.light {\n                .social-block {\n                    &__success {\n                        right: auto;\n                        left: 50%;\n                        transform: translateX(-50%);\n                    }\n\n                    &__share {\n                        margin-right: 0;\n                    }\n                }\n            }\n        }\n    }\n\n    @media (max-width: 640px) {\n        .social-block {\n\n            &__success {\n                left: 50%;\n                transform: translateX(-50%);\n            }\n\n            &__share {\n                margin-right: 6px;\n                margin-bottom: 0;\n            }\n\n            &.light {\n                .social-block__share {\n                    margin-right: 0;\n                    margin-bottom: 9px;\n                }\n            }\n\n            &__list {\n                & > * {\n                    &:not(:last-child) {\n                        margin-right: 22px;\n                    }\n                }\n            }\n\n            &_absolute {\n                right: auto;\n                left: 50%;\n                bottom: 25px;\n                transform: translateX(-50%) !important;\n            }\n        }\n    }\n</style>\n\n\n"]}]}