'.laptop__bullets',\\n                    clickable: true\\n                },\\n                paginationClickable :true,\\n                grabCursor: true,\\n                loop: true,\\n                simulateTouch : false,\\n            },\\n            pauseSliderCondition: false,\\n            progressWidth: 1,\\n            timer: null\\n        }\\n    },\\n    mounted(){\\n        this.$nextTick(() => {\\n            this.swiper.controller.control = this.swiperSmall;\\n            this.swiperSmall.controller.control = this.swiper;\\n        });\\n    },\\n    computed: {\\n        swiperSmall(){\\n            return this.$refs.swiperSmall.swiper;\\n        },\\n        swiper(){\\n            return this.$refs.swiper.swiper;\\n        }\\n    },\\n    methods: {\\n        initSlider(){\\n            let autoplayTime = 3000,\\n                item = 0;\\n\\n            this.timer = setInterval(()=>{\\n                if(!this.pauseSliderCondition) {\\n                    item += autoplayTime / 100;\\n                    this.progressWidth += 1;\\n                    if(item === autoplayTime) {\\n                        if(this.swiperSmall.destroyed !== true) {   //check when changing route\\n                            this.swiperSmall.slideNext();\\n                            item = 0;\\n                            this.progressWidth = 0;\\n                        }\\n                    }\\n                } else {\\n                    window.clearInterval(this.timer);\\n                }\\n            }, autoplayTime / 100);\\n        },\\n        pauseSlider(){\\n            this.pauseSliderCondition = true;\\n        },\\n        startSlider(){\\n            this.pauseSliderCondition = false;\\n        },\\n        slideTo(direction){\\n            clearInterval(this.timer);\\n            this.progressWidth = 0;\\n            this.initSlider();\\n            this.pauseSliderCondition = false;\\n            direction == 1 ? this.swiperSmall.slideNext() : this.swiperSmall.slidePrev();\\n        },\\n        scrollTo(){\\n            let elem = document.getElementById('view');\\n\\n            window.scrollTo({\\n                top: elem.getBoundingClientRect().top + pageYOffset,\\n                behavior: 'smooth'\\n            });\\n        }\\n    },\\n}\\n</script>\\n\\n<style lang=\\\"scss\\\" scoped>\\n    .poster {\\n        background-repeat: no-repeat;\\n        background-position: right top, center;\\n        background-size: 40%, cover;\\n        margin: 10px 10px 0 10px;\\n\\n        &__top {\\n            background: url('../../assets/img/main-bg-pattern.png'),\\n                        linear-gradient(174.64deg, #EBF1FD -30.68%, #DAEEF6 187.69%);\\n            background-repeat: no-repeat;\\n            background-size: 30%, 100%;\\n            background-position: right top, center;\\n            padding: 0 0 32px 87px;\\n            padding-top: 90px;\\n            border-radius: 8px 8px 0 0;\\n            position: relative;\\n        }\\n\\n        &__bottom {\\n            text-align: center;\\n            padding-bottom: 32px;\\n\\n            img {\\n                width: 52%;\\n                height: auto;\\n                margin-top: 8px;\\n            }\\n        }\\n\\n        &__scroll {\\n            margin: 0 auto;\\n            margin-top: 0;\\n            display: block;\\n            position: relative;\\n            z-index: 2;\\n        }\\n\\n        &__text {\\n            margin-bottom: 10px;\\n            font-family: 'SFProDisplay-Regular';\\n            font-size: 12px;\\n            line-height: 19px;\\n            letter-spacing: 1px;\\n            color: #3D4950;\\n            cursor: pointer;\\n            position: relative;\\n        }\\n\\n        &__line {\\n            display: inline-block;\\n            width: 1px;\\n            height: 28px;\\n            background: #727C88;\\n            opacity: 0.2;\\n        }\\n    }\\n\\n    .laptop{\\n        posi