     });\\n    },\\n    methods: {\\n        initSlider(){\\n            let autoplayTime = 3000,\\n                item = 0;\\n\\n            this.timer = setInterval(()=>{\\n                if(!this.pauseSliderCondition) {\\n                    item += autoplayTime / 100;\\n                    this.progressWidth += 1;\\n                    if(item === autoplayTime) {\\n                        this.$refs.swiperSmall.swiper.slideNext();\\n                        item = 0;\\n                        this.progressWidth = 0;\\n                    }\\n                } else {\\n                    window.clearInterval(this.timer);\\n                }\\n            }, autoplayTime / 100);\\n        },\\n        pauseSlider(){\\n            this.pauseSliderCondition = true;\\n        },\\n        startSlider(){\\n            this.pauseSliderCondition = false;\\n        },\\n        slideTo(direction){\\n            clearInterval(this.timer);\\n            this.progressWidth = 0;\\n            this.initSlider();\\n            this.pauseSliderCondition = false;\\n            direction == 1 ? this.$refs.swiperSmall.swiper.slideNext() : this.$refs.swiperSmall.swiper.slidePrev();\\n        },\\n        scrollTo(){\\n            let elem = document.getElementById('view');\\n\\n            window.scrollTo({\\n                top: elem.getBoundingClientRect().top + pageYOffset,\\n                behavior: 'smooth'\\n            });\\n        }\\n    },\\n}\\n</script>\\n\\n<style lang=\\\"scss\\\" scoped>\\n    .poster {\\n        background-repeat: no-repeat;\\n        background-position: right top, center;\\n        background-size: 40%, cover;\\n        margin: 10px 10px 0 10px;\\n\\n        &__top {\\n            background: url('../../assets/img/main-bg-pattern.png'),\\n                        linear-gradient(174.64deg, #EBF1FD -30.68%, #DAEEF6 187.69%);\\n            background-repeat: no-repeat;\\n            background-size: 30%, 100%;\\n            background-position: r